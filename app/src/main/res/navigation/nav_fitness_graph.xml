        tools:layout="@layout/fragment_connected_apps" />
    <fragment
        android:id="@+id/improve_aktivo_score"
        android:name="com.csform.android.MB360.fitness.ImproveAktivoScoreFragment"
        android:label="Improve Aktivo Score"
        tools:layout="@layout/improve_aktivo_score" />

    <fragment
        android:id="@+id/help_fragment"
        android:name="com.csform.android.MB360.fitness.HelpFragment"
        android:label="Help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/privacy_policy_fragment"
        android:name="com.csform.android.MB360.fitness.PrivacyPolicyFragment"
