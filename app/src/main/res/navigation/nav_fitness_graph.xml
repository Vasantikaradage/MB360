<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_fitness_graph"
    app:startDestination="@id/fitnessHomeFragment">

    <fragment
        android:id="@+id/fitnessHomeFragment"
        android:name="com.csform.android.MB360.fitness.FitnessHomeFragment"
        android:label="Home"
        tools:layout="@layout/fitness_home_fragment" />

    <fragment
        android:id="@+id/fitnessProfileFragment"
        android:name="com.csform.android.MB360.fitness.ProfileScreenData.FitnessProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_fitness_profile" />
    <fragment
        android:id="@+id/fitnessCompleteFragment"
        android:name="com.csform.android.MB360.fitness.FitnessCompeteFragment"
        android:label="Compete"
